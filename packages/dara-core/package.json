{
  "name": "@darajs/core",
  "version": "1.10.6",
  "description": "Dara Framework core",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "license": "Apache-2.0",
  "author": "Sam Smith <sam@causalens.com>",
  "homepage": "https://dara.causalens.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/causalens/dara.git",
    "directory": "packages/dara-core"
  },
  "contributors": [
    "Patricia Jacob <patricia@causalens.com>",
    "Krzysztof Bielikowicz <krzysztof@causalens.com>"
  ],
  "scripts": {
    "build": "rimraf dist && rimraf dara/core/umd && vite build && tsc && cp -R js/assets dist/ && cp js/index.css dist/ && tsc-alias && cp -R dist/umd/ dara/core/umd/",
    "build-dev": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "clean": "rimraf api assets components shared types *.js.map *.d.ts '!(*.config).js' *.css",
    "cypress:open": "cypress open",
    "lint": "tsc --noemit && eslint js tests cypress --ext .tsx,.ts --max-warnings 0 && stylelint './js/**/*.tsx'",
    "format": "prettier js --write",
    "format:check": "prettier js --check",
    "lint:fix": "tsc --noemit && eslint js tests cypress --ext .tsx,.ts --max-warnings 0 --fix  && stylelint --fix './js/**/*.tsx'",
    "package": "yarn build && yarn publish --no-git-tag-version",
    "test-watch": "jest --watch",
    "test": "jest",
    "test:e2e": "start-server-and-test 'poetry run dara start --config cypress.dara.main:config' http://127.0.0.1:8000/static/index.js 'cypress run'"
  },
  "prettier": "@darajs/prettier-config",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@darajs/eslint-config": "~1.10.3",
    "@darajs/prettier-config": "~1.10.3",
    "@darajs/stylelint-config": "~1.10.3",
    "@rollup/plugin-inject": "^4.0.4",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/file-saver": "^2.0.5",
    "@types/history": "^4.7.0",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^18.15.3",
    "@types/object-hash": "^2.2.1",
    "@types/react": "^18.2.6",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.2.4",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@types/recoil": "^0.0.9",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "^5.1.23",
    "@vitejs/plugin-react": "2.1.0",
    "babel-jest": "^29.5.0",
    "concurrently": "^8.0.1",
    "cypress": "9.5.3",
    "jest": "^29.5.0",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-websocket-mock": "^2.5.0",
    "msw": "1.0.1",
    "postcss": "^8.4.35",
    "prettier": "^3.0.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^15.0.0",
    "tsc-alias": "^1.8.5",
    "vite": "3.1.8",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@darajs/styled-components": "~1.10.3",
    "@darajs/ui-components": "~1.10.3",
    "@darajs/ui-notifications": "~1.10.3",
    "@darajs/ui-utils": "~1.10.3",
    "@fortawesome/fontawesome-free": "~6.4.0",
    "@recoiljs/refine": "^0.1.1",
    "@tanstack/query-core": "^4.0.0",
    "@tanstack/react-query": "^4.0.0",
    "date-fns": "2.9.0",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "history": "^4.9.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "polished": "^4.1.1",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.0",
    "react-router": "^5.3.0",
    "react-router-dom": "^5.3.0",
    "react-virtualized-auto-sizer": "1.0.6",
    "react-window": "^1.8.6",
    "recoil": "^0.7.7",
    "recoil-sync": "^0.2.0",
    "rxjs": "^7.0.0",
    "shortid": "2.2.15",
    "styled-components": "^5.3.10",
