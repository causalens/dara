{
  "name": "@darajs/core",
  "version": "1.25.1",
  "description": "Dara Framework core",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "license": "Apache-2.0",
  "author": "Sam Smith <sam@causalens.com>",
  "homepage": "https://dara.causalens.com/",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/causalens/dara.git",
    "directory": "packages/dara-core"
  },
  "contributors": [
    "Patricia Jacob <patricia@causalens.com>",
    "Krzysztof Bielikowicz <krzysztof@causalens.com>"
  ],
  "scripts": {
    "build": "rimraf dist && rimraf dara/core/umd && vite build && tsc && cp -R js/assets dist/ && cp js/index.css dist/ && tsc-alias && cp -R dist/umd/. dara/core/_assets/auto_js/",
    "prepare-dev": "mkdir -p dist && tsc && cp -R js/assets dist/ && cp js/index.css dist/ && tsc-alias",
    "build-dev": "mkdir -p dist && cp js/index.css dist/ && cp -R js/assets dist/ && tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "clean": "rimraf api assets components shared types *.js.map *.d.ts '!(*.config).js' *.css",
    "cypress:open": "cypress open",
    "lint": "tsc --noemit && eslint js tests cypress --ext .tsx,.ts --max-warnings 0 && stylelint './js/**/*.tsx'",
    "format": "prettier js --write --list-different",
    "format:check": "prettier js --check",
    "lint:fix": "tsc --noemit && eslint js tests cypress --ext .tsx,.ts --max-warnings 0 --fix  && stylelint --fix './js/**/*.tsx'",
    "package": "yarn build && yarn publish --no-git-tag-version",
    "test-watch": "vitest",
    "test": "vitest --run",
    "test:e2e": "start-server-and-test 'poetry run dara start --config cypress.dara.main:config' http://127.0.0.1:8000/static/index.js 'cypress run'"
  },
  "prettier": "@darajs/prettier-config",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@darajs/eslint-config": "1.25.1",
    "@darajs/prettier-config": "1.25.1",
    "@darajs/stylelint-config": "1.25.1",
    "@rollup/plugin-inject": "^4.0.4",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/file-saver": "^2.0.5",
    "@types/lodash": "^4.14.178",
    "@types/node": "^18.15.3",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.2.6",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^18.2.4",
    "@types/react-virtualized-auto-sizer": "1.0.4",
    "@types/recoil": "^0.0.9",
    "@types/styled-components": "^5.1.23",
    "@types/whatwg-fetch": "^0.0.33",
    "@vitejs/plugin-react": "4.6.0",
    "concurrently": "^8.0.1",
    "cypress": "9.5.3",
    "msw": "^2.10.5",
    "postcss": "^8.4.35",
    "prettier": "^3.0.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^2.1.3",
    "stylelint": "^15.0.0",
    "tsc-alias": "^1.8.5",
    "vite": "7.0.8",
    "vitest": "^3.2.4",
    "vitest-websocket-mock": "^0.5.0",
    "whatwg-fetch": "^3.6.20"
  },
  "dependencies": {
    "@darajs/styled-components": "1.25.1",
    "@darajs/ui-components": "1.25.1",
    "@darajs/ui-notifications": "1.25.1",
    "@darajs/ui-utils": "1.25.1",
    "@fortawesome/fontawesome-free": "~6.4.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@recoiljs/refine": "^0.1.1",
    "@tanstack/query-core": "^4.40.0",
    "@tanstack/react-query": "^4.40.1",
    "date-fns": "2.30.0",
    "exceljs": "^4.3.0",
    "fast-json-patch": "^3.1.1",
    "file-saver": "^2.0.5",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.0",
    "nprogress": "^0.2.0",
    "polished": "^4.1.1",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.0",
    "react-router": "~7.12.0",
    "react-virtualized-auto-sizer": "1.0.6",
    "react-window": "^1.8.6",
    "recoil": "^0.7.7",
    "recoil-sync": "^0.2.0",
    "rxjs": "^7.0.0",
    "styled-components": "^5.3.10",
    "typescript": "^5.0.4",
    "use-async-resource": "^2.2.2",
    "zod": "^3.25"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}
