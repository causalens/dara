{
  "extends": "./tsconfig.json",
  "include": [
    "./js/",
    "./tests/",
    "./cypress/"
  ],
  "exclude": [],
  "compilerOptions": {
    "rootDir": ".",
    "paths": {
      "@/*": [
        "./js/*"
      ]
    },
    "noImplicitAny": true,
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true
  }
}
