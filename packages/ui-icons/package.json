{
  "name": "@darajs/ui-icons",
  "version": "1.21.6",
  "description": "Icons for the Dara UI framework",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prepare-dev": "tsc",
    "lint": "eslint src --ext .tsx --max-warnings 0 --fix && stylelint './src/**/*.tsx' --fix",
    "format": "prettier src --write",
    "format:check": "prettier src --check",
    "test-watch": "jest --watch",
    "generate": "node ./scripts/generate.js"
  },
  "author": "Krzysztof Bielikowicz <krzysztof@causalens.com>",
  "license": "Apache-2.0",
  "prettier": "@darajs/prettier-config",
  "devDependencies": {
    "@darajs/eslint-config": "1.21.6",
    "@darajs/prettier-config": "1.21.6",
    "@darajs/stylelint-config": "1.21.6",
    "@types/jest": "^25.2.3",
    "@types/react": "18.2.60",
    "@types/styled-components": "^5.1.26",
    "eslint": "^8.57.1",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "stylelint": "^15.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@darajs/styled-components": "1.21.6",
    "@fortawesome/fontawesome-svg-core": "~6.4.0",
    "@fortawesome/free-brands-svg-icons": "~6.4.0",
    "@fortawesome/free-regular-svg-icons": "~6.4.0",
    "@fortawesome/free-solid-svg-icons": "~6.4.0",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@mdi/js": "5.9.55",
    "@mdi/react": "1.4.0",
    "react": "^18.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
