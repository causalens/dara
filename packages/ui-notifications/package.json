{
  "name": "@darajs/ui-notifications",
  "version": "1.21.2",
  "description": "Notification system for the Dara UI Framework",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "prepare-dev": "tsc",
    "lint": "eslint src --ext .tsx --max-warnings 0 && stylelint './src/**/*.tsx'",
    "format": "prettier src --write",
    "format:check": "prettier src --check",
    "lint:fix": "eslint src --ext .tsx --max-warnings 0 --fix && stylelint './src/**/*.tsx' --fix",
    "test-watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "author": "Krzysztof Bielikowicz <krzysztof@causalens.com>",
  "license": "Apache-2.0",
  "prettier": "@darajs/prettier-config",
  "devDependencies": {
    "@darajs/eslint-config": "1.21.2",
    "@darajs/prettier-config": "1.21.2",
    "@darajs/stylelint-config": "1.21.2",
    "@storybook/react-vite": "^9.0.17",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.155",
    "@types/react": "^18.0",
    "@types/styled-components": "^5.1.26",
    "eslint": "^8.57.1",
    "eslint-plugin-storybook": "9.0.17",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "^17.0",
    "rimraf": "^3.0.2",
    "storybook": "^9.0.17",
    "storybook-dark-mode": "^2.1.1",
    "stylelint": "^15.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@darajs/styled-components": "1.21.2",
    "@darajs/ui-icons": "1.21.2",
    "@darajs/ui-utils": "1.21.2",
    "lodash": "4.17.21",
    "polished": "^4.1.1",
    "react": "^18.0",
    "rxjs": "^7.0.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
