{
  "name": "@darajs/ui-hierarchy-viewer",
  "version": "1.12.2",
  "description": "Hierarchy Viewer for the Dara UI framework",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "eslint src --ext .tsx --max-warnings 0 && stylelint './src/**/*.tsx'",
    "format": "prettier src --write",
    "format:check": "prettier src --check",
    "lint:fix": "eslint src --ext .tsx --max-warnings 0 --fix && stylelint './src/**/*.tsx' --fix",
    "test-watch": "jest --watch"
  },
  "author": "Krzysztof Bielikowicz <krzysztof@causalens.com>",
  "license": "Apache-2.0",
  "prettier": "@darajs/prettier-config",
  "devDependencies": {
    "@darajs/eslint-config": "1.12.2",
    "@darajs/prettier-config": "1.12.2",
    "@darajs/stylelint-config": "1.12.2",
    "@types/d3": "^6.2.0",
    "@types/jest": "^25.2.3",
    "@types/react": "^18.0",
    "@types/styled-components": "^5.1.26",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "^17.0",
    "rimraf": "^3.0.2",
    "stylelint": "^15.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@darajs/styled-components": "1.12.2",
    "@darajs/ui-utils": "1.12.2",
    "d3": "6.2.0",
    "react": "^18.0"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
