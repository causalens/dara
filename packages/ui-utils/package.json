{
  "name": "@darajs/ui-utils",
  "version": "1.10.3",
  "description": "General utilities for the Dara UI framework",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "eslint src --ext .tsx --max-warnings 0 && stylelint './src/**/*.tsx'",
    "format": "prettier src --write",
    "format:check": "prettier src --check",
    "lint:fix": "eslint src --ext .tsx --max-warnings 0 --fix && stylelint './src/**/*.tsx' --fix",
    "test-watch": "jest --watch"
  },
  "author": "Krzysztof Bielikowicz <krzysztof@causalens.com>",
  "license": "Apache-2.0",
  "prettier": "@darajs/prettier-config",
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@darajs/eslint-config": "^1.10.3",
    "@darajs/prettier-config": "^1.10.3",
    "@darajs/stylelint-config": "^1.10.3",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.155",
    "@types/react": "^18.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.32.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "^17.0",
    "rimraf": "^3.0.2",
    "stylelint": "^15.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "d3-scale": "3.2.1",
    "lodash": "4.17.21",
    "react": "^18.0",
    "rxjs": "^7.0.0",
    "shortid": "2.2.15"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
